<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="scanned_docs_frontend">

    <configlet
        title="Scanned Docs Frontend settings"
        action_id="scanned_docs_frontend.settings"
        appId="scanned_docs_frontend"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@scanned_docs_controlpanel"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
